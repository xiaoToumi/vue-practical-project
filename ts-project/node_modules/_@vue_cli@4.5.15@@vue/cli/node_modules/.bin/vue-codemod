#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_vue-codemod@0.0.5@vue-codemod/dist/bin/vue-codemod.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_vue-codemod@0.0.5@vue-codemod/dist/bin/vue-codemod.js" "$@"
  ret=$?
fi
exit $ret
